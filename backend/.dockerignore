.git/
.gitignore
.gitattributes

# Documentation
README.md
*.md
docs/

# Test results
TestResults/
coverage/
.nyc_output
.mocha.tmp

# Node.js (if any frontend assets)
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Temporary files
*.tmp
*.temp
temp/

# IDE files
.idea/
*.iml
